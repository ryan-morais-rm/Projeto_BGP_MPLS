# Configuração de interface e IP

hostname PE-AS10
!
int lo0
ip add 1.1.10.1 255.255.255.255
no shut
!
int ethernet0/0
ip add 10.64.10.1 255.255.255.252
no shut
!
int ethernet 0/1
ip add 10.64.11.1 255.255.255.252
no shut
!
int ethernet 0/2
ip add 10.0.12.1 255.255.255.252
no shut
!
int ethernet 0/3
ip add 10.0.13.1 255.255.255.252
no shut
